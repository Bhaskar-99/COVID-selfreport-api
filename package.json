{
  "name": "covid-selfreport-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-sources": "babel --source-maps --copy-files ./*.js --out-dir ./dist",
    "build-lib": "babel --source-maps --copy-files lib --out-dir ./dist/lib",
    "build-routes": "babel --source-maps --copy-files routes --out-dir ./dist/routes",
    "build-scripts": "babel --source-maps --copy-files scripts --out-dir ./dist/scripts",
    "build-all": "npm run build-sources && npm run build-scripts && npm run build-routes && npm run build-lib",
    "build": "npm run clean && npm run copy && npm run build-all",
    "clean": "rm -rf dist && mkdir dist && mkdir dist/logs",
    "copy": "rsync -r --exclude=.git --exclude=app --exclude=.idea --exclude=tests --exclude=node_modules --exclude=dist --exclude=logs --delete . dist",
    "start": "node server.js",
    "start:dev": "nodemon --exec npx babel-node -- server.js | bunyan",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/register": "^7.0.0-rc.1",
    "@babel/runtime": "^7.0.0-rc.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "co": "^4.6.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-rate-limit": "^4.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "mongoose": "^5.5.11",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "on-response": "^1.0.1",
    "p3p": "0.0.2",
    "rate-limit-redis": "^1.6.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "latest",
    "seraph": "^0.15.3",
    "sha1": "^1.1.1",
    "slugify": "^1.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "nodemon": "^1.18.3"
  }
}
